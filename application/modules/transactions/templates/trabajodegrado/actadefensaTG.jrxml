			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString language="SQL">
			<![CDATA[Select u.pk_usuario, (u.apellido ||', '||u.nombre) as evaluador, ta.valor as rol
from tbl_tutorestesis as ttt
join tbl_usuariosgrupos as tug on ttt.fk_usuariogrupo = tug.pk_usuariogrupo
join tbl_usuarios as u on tug.fk_usuario = u.pk_usuario
join tbl_atributos as ta on ttt.fk_tipo = ta.pk_atributo
where fk_datotesis = $P{cod}::integer and ttt.fk_periodo = $P{periodo}::integer]]>
		</queryString>
		<field name="pk_usuario" class="java.lang.Long"/>
		<field name="evaluador" class="java.lang.String"/>
		<field name="rol" class="java.lang.String"/>
	</subDataset>
	<parameter name="cod" class="java.lang.String"/>
	<parameter name="periodo" class="java.lang.String"/>
	<parameter name="logo" class="java.lang.String"/>
	<queryString>
		<![CDATA[select distinct dt.pk_datotesis,
	deft.fecha,
	ho.horainicio,
	e.nombre as aula,
	e1.nombre as edificio,
	e2.nombre as sede,
	dt.titulo
from tbl_datostesis		dt
join tbl_defensastesis		deft	on	deft.fk_datotesis 		= 	dt.pk_datotesis
join tbl_horarios		ho	on	ho.pk_horario 			= 	deft.fk_horario
join tbl_estructuras		e	on 	e.pk_estructura			=	deft.fk_estructura
join tbl_estructuras		e1	on 	e1.pk_estructura		=	e.fk_estructura
join tbl_estructuras		e2	on 	e2.pk_estructura		=	e1.fk_estructura
join tbl_autorestesis		at 	on 	at.fk_Datotesis 		=	dt.pk_datotesis 	and at.renuncia = false
where dt.pk_datotesis = $P{cod}::integer]]>
	</queryString>
