	<queryString>
		<![CDATA[SELECT CASE WHEN sqt.requisito = 'f' THEN '>>>' ELSE '' END, sqt.codigopropietario , sqt.materia, sqt.id, sqt.unidadcredito, sqt.seccion
FROM
(
SELECT
       at.codigopropietario,
       m.materia,
       sm1.id,
       at.unidadcredito,
       (SELECT COALESCE((SELECT sc.valor
        FROM tbl_asignaciones ag1
        INNER JOIN vw_secciones sc ON sc.pk_atributo = ag1.fk_seccion
        WHERE ra.fk_asignacion = ag1.pk_asignacion), '')) AS seccion,
       (SELECT CASE WHEN (array_upper(ARRAY(SELECT fk_asignaturaprelada
                                              FROM tbl_prelaciones
                                              WHERE fk_asignatura = ra.fk_asignatura
                                                AND fk_asignaturaprelada <> 0), 1) IS NULL)
                      THEN TRUE
                      WHEN (SELECT COUNT(pk_inscripcion)
                            FROM tbl_recordsacademicos ra1
                            INNER JOIN tbl_inscripciones  i  ON i.pk_inscripcion   = ra1.fk_inscripcion
                            INNER JOIN tbl_usuariosgrupos ug ON ug.pk_usuariogrupo = i.fk_usuariogrupo
                            WHERE calificacion >= 10
                              AND ug.fk_usuario = $P!{ci}
                              AND fk_asignatura = ANY(ARRAY(SELECT fk_asignaturaprelada
                                                            FROM tbl_prelaciones
                                                            WHERE fk_asignatura = ra.fk_asignatura))) = (array_upper(ARRAY(SELECT fk_asignaturaprelada
                                                                                                                          FROM tbl_prelaciones
                                                                                                                          WHERE fk_asignatura = ra.fk_asignatura), 1))
                      THEN true
                      WHEN ARRAY(SELECT fk_asignaturaprelada
                                 FROM tbl_prelaciones
                                 WHERE fk_asignatura = ra.fk_asignatura) IS NULL THEN true

                      ELSE false
                 END) AS requisito
FROM tbl_inscripciones            i
LEFT JOIN tbl_usuariosgrupos    ug ON ug.pk_usuariogrupo = i.fk_usuariogrupo
LEFT JOIN tbl_usuarios           u ON  u.pk_usuario      = ug.fk_usuario
LEFT JOIN tbl_estructuras        e ON  e.pk_estructura   = i.fk_estructura
LEFT JOIN tbl_recordsacademicos ra ON ra.fk_inscripcion  = i.pk_inscripcion
LEFT JOIN tbl_asignaciones      ag ON ag.pk_asignacion   = ra.fk_asignacion
LEFT JOIN tbl_asignaturas       at ON at.pk_asignatura   = ag.fk_asignatura
LEFT JOIN tbl_periodos           p ON  p.pk_periodo      = i.fk_periodo
LEFT JOIN vw_materias            m ON  m.pk_atributo     = at.fk_materia
LEFT JOIN vw_semestres         sm1 ON sm1.pk_atributo     = ag.fk_semestre
LEFT JOIN vw_semestres         sm2 ON sm2.pk_atributo     = i.fk_semestre
LEFT JOIN vw_escuelas           es ON es.pk_atributo     = i.fk_atributo
LEFT JOIN tbl_pensums pe ON pe.pk_pensum = i.fk_pensum
WHERE ug.fk_usuario = $P!{ci}
  AND  i.fk_periodo = (SELECT fn_xrxx_reinscripcion_upi(ug.fk_usuario))
  ) as sqt
  ORDER BY 2]]>
