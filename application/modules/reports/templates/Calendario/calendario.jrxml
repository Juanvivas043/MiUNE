	<subDataset name="New Dataset 1" uuid="1d5eb433-72a7-46c4-92f3-ae3596d63723">
		<parameter name="parameter1" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT consecutivo,subconsec,
CASE WHEN subconsec is null THEN null ELSE actividad END as actividad,
CASE WHEN subconsec is null THEN null ELSE fechainicio END as fechainicio,
CASE WHEN subconsec is null THEN null ELSE fehafin END as fehafin
FROM fn_xrxx_get_calendario(122) as (consecutivo varchar, subconsec varchar, actividad varchar, fechainicio date, fehafin date)
where subconsec ilike 2 || '.%']]>
		</queryString>
		<field name="consecutivo" class="java.lang.String"/>
		<field name="subconsec" class="java.lang.String"/>
		<field name="actividad" class="java.lang.String"/>
		<field name="fechainicio" class="java.sql.Date"/>
		<field name="fehafin" class="java.sql.Date"/>
	</subDataset>
	<subDataset name="semanas" uuid="b3534b04-f337-4238-9709-fea55ab500f4">
		<parameter name="sem_periodo" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT * FROM fn_xrxx_obtener_semanas(
  (SELECT fechainicio::timestamp
  FROM tbl_calendarios
  where fk_actividad = 1641
      and fk_periodo = $P!{sem_periodo}),
  (SELECT fechainicio::timestamp
  FROM tbl_calendarios
  where fk_actividad =1679
      and fk_periodo = $P!{sem_periodo})) as (a varchar,b integer)]]>
		</queryString>
		<field name="a" class="java.lang.String"/>
		<field name="b" class="java.lang.Integer"/>
		<variable name="total_semanas" class="java.lang.Integer" calculation="Sum">
			<variableExpression><![CDATA[$F{b}]]></variableExpression>
