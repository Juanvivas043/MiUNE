	</subDataset>
	<subDataset name="dataset1" uuid="4809d7d1-5442-4281-8a6d-b599be8902cd"/>
	<subDataset name="periodo" uuid="b01a4400-4659-4f39-9686-9fda001e0ae3">
		<parameter name="periodo_periodo" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT to_char(inicioclases, 'TMMonth') || ' - ' || to_char(fechafin, 'TMMonth') || ' ' || extract(year from fechafin) as fechas
FROM tbl_periodos
WHERE pk_periodo = $P!{periodo_periodo}]]>
		</queryString>
		<field name="fechas" class="java.lang.String"/>
	</subDataset>
