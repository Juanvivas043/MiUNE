		<defaultValueExpression><![CDATA["/Users/pocho/Sites/MiUNE2-interfaz/public/images/logo_UNE_color.jpg"]]></defaultValueExpression>
	</parameter>
	<parameter name="cantidad_subs" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[$F{subcount}]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT consecutivo,
       actividad,
       CASE WHEN fechainicio <> fehafin
	  THEN CASE WHEN mesinicio <> mesfin
                      THEN
diainicio || ' de ' || mesinicio || ' al ' || diafin || ' de ' || mesfin
		  ELSE diainicio || ' al ' || diafin || ' de ' || mesinicio END
            ELSE diainicio || ' de ' || mesinicio END AS fecha,
subcount,
destacar

FROM(
SELECT consecutivo,
actividad,
to_char(fechainicio, 'DD') as diainicio,
CASE to_char(fechainicio, 'TMMonth')
WHEN 'May' THEN 'Mayo' ELSE to_char(fechainicio, 'TMMonth')
END AS mesinicio,
to_char(fehafin, 'DD') as diafin,
CASE to_char(fehafin, 'TMMonth')
WHEN 'May' THEN 'Mayo'ELSE to_char(fehafin, 'TMMonth')
END AS mesfin,
fechainicio,
fehafin,
subcount,
destacar
FROM fn_xrxx_get_calendario($P!{Periodo}) as (consecutivo varchar, subconsec varchar, actividad varchar, fechainicio date, fehafin date, subcount int,destacar boolean)
where subconsec is null
)as sqt]]>
	</queryString>
